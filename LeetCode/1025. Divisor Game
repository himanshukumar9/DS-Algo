class Solution {
    public boolean divisorGame(int n) {
    // O(1) solution
        //  if(n%2==0) return true;
       // else return false;
 
        // O(n) solution

        if (n==1) return false;
        if (n==2) return true;
       
        for (int i=1;i<=n;i++){
            if (n%i == 0)
                return !(divisorGame(n-i));
       }
        return false;
  
    }
}
